@{
    Layout = null;
}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>后台首页</title>
    <style type="text/css">
        * html frame{ overflow:hidden;}
        * html frameset{ overflow:hidden;}
        #frameLeft{ overflow:scroll;}
    </style>
</head>
<frameset rows="75,*,70" border="0" frameborder="0" framespacing="0">
  <frame src="@Url.Action("Navbar")" name="frameTop" scrolling="no" noresize="noresize"/>
  <frameset cols="240,*" border="0" frameborder="0" framespacing="0">
    <frame src="@Url.Action("Menu")" name="frameLeft" scrolling="auto" noresize="noresize"/>
	<frame src="@Url.Action("Runinfo")" name="frameRight" noresize="noresize"/>
  </frameset>
  <frame src="@Url.Action("Bottom")" name="frameTop" scrolling="no" noresize="noresize"/>
</frameset>
<noframes>
</noframes>
</html>
